{
  "version": 2,
  "env": {
    "APP_ID": "@app-id",
    "WEBHOOK_SECRET": "@webhook-secret",
    "PRIVATE_KEY": "@private-key"
  },
  "builds": [
    {
      "src": "src/now.ts",
      "use": "@now/node",
      "config": {
        "includeFiles": ["src/dictionaries/**/*"],
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [{ "src": "/", "dest": "src/now.ts" }]
}
